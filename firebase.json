{
  "hosting": {
    "public": "public",
    "trailingSlash": false,
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [ {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        } ]
      }
    ]
  
  },
  "storage": {
    "rules": "storage.rules"
  }
}
