{
  "rules": {
    "users": {
      "$uid": {
        ".read": "true",
        ".write": "$uid === auth.uid"
      }
    },

    "messages": {
      ".read": "auth != null",
      ".write": "auth != null"
    },

    "usernames": {
      ".read": "auth != null",
      ".write": "auth != null"
    }

  }
}
